@model AdminVM
@{
    ViewData["Title"] = "YeniAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Yeni Admin Ekleme</h1>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x=>x.Kullanici)
    @Html.TextBoxFor(x=>x.Kullanici, new{@class="form-control"})
    <br />
    @Html.LabelFor(x=>x.Sifre)
    @Html.TextBoxFor(x=>x.Sifre, new{@class="form-control"})
    <br />

    <button class="btn btn-primary">Admin Ekle</button>
</form>
